<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="Extract_headers">
    <DisplayName>Extract_headers</DisplayName>
    <Properties/>
    <Header name="Content-Type">
        <Pattern ignoreCase="true">{Content-type}</Pattern>
    </Header>
    <Header name="Accept">
        <Pattern ignoreCase="true">{Accept}</Pattern>
    </Header>
    <Variable name="request.verb">
        <Pattern>{verb}</Pattern>
    </Variable>
</ExtractVariables>